<div class="min-h-screen bg-gray-100 min-w-screen">
    <header class="w-full text-gray-700 bg-white border-t border-gray-100 shadow-sm body-font">
        <div class="container flex flex-col flex-wrap items-center p-5 mx-auto md:flex-row">

            <nav class="flex flex-wrap items-center text-base lg:w-2/5 md:ml-auto">
                <a href="{{ path('app_home') }}" class="mr-5 font-medium hover:text-gray-900">Home</a>
            </nav>
            <div class="inline-flex items-center h-full ml-5 lg:w-2/5 lg:justify-end lg:ml-0">
                {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                    <a href="{{ path('_logout_main') }}" class="mr-5 font-medium hover:text-gray-900">Logout</a>
                    <a href="{{ path('app_admin_dashbord') }}" class="mr-5 font-medium hover:text-gray-900">Dashbord</a>
                    {% else %}
                        <a href="{{ path('app_login') }}" class="mr-5 font-medium hover:text-gray-900">Login</a>
                {% endif %}
            </div>
        </div>
    </header>
</div>